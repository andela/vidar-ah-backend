{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db-migrate": "sequelize db:migrate",
    "db:undo-migration": "sequelize db:migrate:undo:all",
    "lint": "eslint . --ext .js",
    "start": "sequelize db:migrate && node dist/index",
    "start:dev": "babel-watch index.js",
    "build": "rm -rf dist && babel ./ -d ./dist --copy-files --ignore ./node_modules",
    "postinstall": "npm run build",
    "test": "npm run db:undo-migration && npm run db-migrate && nyc --reporter=lcov --reporter=text mocha test/index.js --timeout=20000 --require babel-register --exit"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.4.16",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.8.1",
    "pusher": "^2.2.0",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "shortid": "^2.2.14",
    "slug": "^0.9.1",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.1.4",
    "mochawesome": "^3.1.1",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
